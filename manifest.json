{
  "manifest_version": 2,
  "name": "Check it News",
  "description" : "Inf 134 Chrome Extension",
  "version": "1.0.0",
  "browser_action": {
    "default_icon" : "check_it.png",
    "default_popup" : "check_it.html",
    "default_title" : "Check-it News"
  },
  "content_scripts" : [
    {"matches" : ["<all_urls>"],
    "js": ["thirdParty/jquery-3.3.1.min.js", "content.js"]}
  ],
  "background" : {
    "scripts" : ["thirdParty/jquery-3.3.1.min.js", "background.js"]
  }
}
